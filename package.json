{
  "name": "@telescan/connected-react-router",
  "version": "7.0.0",
  "description": "A Redux binding for React Router v4 and v5",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "author": "Fabian Dr√∂ge",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://proget.intern.zollsoft.org/npm/npm-internal/"
  },
  "files": [
    "*.md",
    "*.js",
    "*.ts",
    "esm",
    "lib",
    "umd"
  ],
  "scripts": {
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js --output umd/ConnectedReactRouter.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack -p src/index.js --output umd/ConnectedReactRouter.min.js",
    "build": "npm run build:esm & npm run build:commonjs & npm run build:umd & npm run build:umd:min",
    "lint": "eslint .",
    "test": "jest --config ./.jestrc.json",
    "test:watch": "npm run test -- --watch",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "lodash.isequalwith": "^4.4.0"
  },
  "peerDependencies": {
    "history": "^5.0.0",
    "react": "^16.4.0 || ^17.0.0 || ^18.2.0",
    "react-redux": "^8.0.0 || ^9.0.0",
    "react-router": "^5.0.0 || ^6.0.0",
    "redux": "^4.0.0 || ^5.0.0"
  },
  "optionalDependencies": {
    "immutable": "^4.0.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@types/history": "^5.0.0",
    "@types/react": "^18.0.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^5.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.6.0",
    "babel-loader": "^9.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-template": "^6.2.0",
    "babel-types": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "immutable": "^4.0.0",
    "jest": "^29.0.0",
    "raf": "^3.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.1.0",
    "react-router": "^6.0.0",
    "react-test-renderer": "^18.0.0",
    "redux": "^5.0.0",
    "redux-immutable": "^4.0.0",
    "redux-mock-store": "^1.2.1",
    "redux-seamless-immutable": "^0.4.0",
    "rewire": "^7.0.0",
    "seamless-immutable": "^7.1.3",
    "webpack": "^5.41.0",
    "webpack-cli": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://git.intern.zollsoft.org/telescan/connected-react-router"
  }
}
